<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.hbpm.core.dao.CoreUserDao">
    <select id="findByUsername" resultMap="com.hbpm.mapper.core.CoreUserBaseMapper.BaseResultMap">
        select
            *
        from
            core_user
        where
            username = #{username}
    </select>
</mapper>