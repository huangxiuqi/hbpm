<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.hbpm.core.dao.CoreOauth2ClientDao">
    <select id="findByClientId" resultMap="com.hbpm.mapper.core.CoreOauth2ClientBaseMapper.BaseResultMap">
        select
            *
        from
            core_oauth2_client
        where
            client_id = #{clientId}
    </select>
</mapper>